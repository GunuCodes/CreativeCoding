function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  fill(0, 0, 255); 
  rect(150, 200, 100, 150);
  
  fill(255, 204, 204); 
  circle(200, 175, 50);
  
  fill(255, 0, 0); 
  triangle(150, 175, 250, 175, 200, 75);
  
  fill(128, 128, 128); 
  ellipse(200, 200, 70, 30);
}
